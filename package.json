{
  "name": "course-booking-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix:chunk-error": "node scripts/fix-chunk-error.js",
    "fix:chunk-cache": "node scripts/fix-chunk-cache.js",
    "postinstall": "prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "create-admin": "ts-node scripts/create-admin.ts",
    "payment-approval:analyze": "ts-node scripts/manage-payment-approval-transition.ts analyze",
    "payment-approval:simulate": "ts-node scripts/manage-payment-approval-transition.ts simulate",
    "payment-approval:report": "ts-node scripts/manage-payment-approval-transition.ts report",
    "test": "tsx scripts/run-tests.ts",
    "test:e2e": "npx playwright test",
    "test:integration": "jest",
    "test:performance": "tsx scripts/test-performance.ts",
    "test:all": "tsx scripts/run-tests.ts",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report",
    "test:install": "npx playwright install",
    "test:mobile": "playwright test --config=playwright.config.mobile.ts",
    "test:fast": "playwright test --config=playwright.config.fast.ts tests/fast-core.spec.ts --timeout=15000 --reporter=line",
    "test:batch1": "playwright test --config=playwright.config.fast.ts tests/batch-1-auth.spec.ts --timeout=15000 --reporter=line",
    "test:batch2": "playwright test --config=playwright.config.fast.ts tests/batch-2-navigation.spec.ts --timeout=15000 --reporter=line",
    "test:batch3": "playwright test --config=playwright.config.fast.ts tests/batch-3-mobile.spec.ts --timeout=15000 --reporter=line",
    "test:mobile:ui": "playwright test --config=playwright.config.mobile.ts --ui",
    "test:mobile:debug": "playwright test --config=playwright.config.mobile.ts --debug",
    "test:mobile:headed": "playwright test --config=playwright.config.mobile.ts --headed",
    "test:mobile:report": "playwright test --config=playwright.config.mobile.ts && playwright show-report test-results/mobile",
    "test:mobile:all": "tsx scripts/run-mobile-tests.ts",
    "test:mobile:quick": "tsx scripts/run-mobile-tests.ts --devices 'iPhone SE,Galaxy S8,Desktop Chrome'",
    "test:mobile:performance": "tsx scripts/run-mobile-tests.ts --tests 'performance'",
    "test:mobile:accessibility": "tsx scripts/run-mobile-tests.ts --tests 'accessibility'",
    "test:mobile:responsive": "tsx scripts/run-mobile-tests.ts --tests 'responsive'",
    "test:mobile:offline": "tsx scripts/run-mobile-tests.ts --tests 'offline'",
    "test:mobile:touch": "tsx scripts/run-mobile-tests.ts --tests 'touch'",
    "test:mobile:service-worker": "tsx scripts/run-mobile-tests.ts --tests 'service-worker'",
    "test:mobile:push-notifications": "tsx scripts/run-mobile-tests.ts --tests 'push-notifications'",
    "test:mobile:background-sync": "tsx scripts/run-mobile-tests.ts --tests 'background-sync'",
    "test:mobile:preloading": "tsx scripts/run-mobile-tests.ts --tests 'preloading'",
    "test:mobile:caching": "tsx scripts/run-mobile-tests.ts --tests 'caching'",
    "test:mobile:analytics": "tsx scripts/run-mobile-tests.ts --tests 'analytics'",
    "test:mobile:gestures": "tsx scripts/run-mobile-tests.ts --tests 'gestures'",
    "test:mobile:pwa": "tsx scripts/run-mobile-tests.ts --tests 'pwa'",
    "test:mobile:compatibility": "tsx scripts/run-mobile-tests.ts --tests 'compatibility'",
    "test:mobile:regression": "tsx scripts/run-mobile-tests.ts --tests 'regression'",
    "test:mobile:smoke": "tsx scripts/run-mobile-tests.ts --devices 'iPhone SE,Desktop Chrome' --tests 'smoke'",
    "test:mobile:full": "tsx scripts/run-mobile-tests.ts --devices 'iPhone SE,iPhone 12 Pro Max,Galaxy S8,Galaxy S20,iPad,Desktop Chrome,Desktop Safari,Desktop Firefox'",
    "test:mobile:ci": "tsx scripts/run-mobile-tests.ts --devices 'iPhone SE,Galaxy S8,Desktop Chrome' --parallel --retries 3",
    "test:mobile:debug-device": "playwright test --config=playwright.config.mobile.ts --project='iPhone SE' --debug",
    "test:mobile:debug-test": "playwright test --config=playwright.config.mobile.ts --grep='should load homepage correctly' --debug",
    "test:mobile:update-snapshots": "playwright test --config=playwright.config.mobile.ts --update-snapshots",
    "test:mobile:codegen": "playwright codegen --config=playwright.config.mobile.ts",
    "test:mobile:install": "playwright install",
    "test:mobile:install-browsers": "playwright install-deps",
    "test:mobile:clean": "rm -rf test-results/mobile && rm -rf playwright-report",
    "test:mobile:clean-all": "rm -rf test-results && rm -rf playwright-report && rm -rf node_modules/.cache",
    "test:mobile:setup": "npm run test:mobile:install && npm run test:mobile:install-browsers",
    "test:batch:split": "node scripts/batch-tests.js",
    "bug:detect": "tsx scripts/run-bug-detection.ts",
    "bug:fix": "tsx scripts/run-bug-detection.ts && echo 'Review the bug-detection-report.json file for issues to fix'",
    "bug:auto-fix": "tsx scripts/auto-fix-common-issues.ts",
    "bug:fix-console-errors": "tsx scripts/fix-console-errors.ts",
    "bug:fix-server-errors": "tsx scripts/fix-server-console-errors.ts",
    "fix:unused-imports": "tsx scripts/fix-unused-imports.ts",
    "fix:dev-server": "tsx scripts/fix-dev-server.ts",
    "test:comprehensive": "npx playwright test tests/comprehensive-bug-detection.spec.ts --reporter=html",
    "error:scan": "tsx scripts/fix-common-errors.ts",
    "error:fix": "tsx scripts/fix-specific-errors.ts",
    "error:fix-critical": "tsx scripts/fix-critical-errors.ts",
    "db:test": "tsx scripts/test-db-connection.ts",
    "db:warmup": "tsx scripts/warmup-db.ts",
    "dev:warmup": "node scripts/dev-with-warmup.js",
    "test:stats": "tsx scripts/test-stats-api.ts",
    "test:warmup": "tsx scripts/test-server-warmup.ts",
    "test:fresh": "tsx scripts/test-fresh-visit.ts",
    "test:display": "tsx scripts/test-frontend-display.ts",
    "test:zero-flash": "tsx scripts/test-zero-flash.ts",
    "fix:search-bars": "tsx scripts/fix-search-bars.ts",
    "fix:search-bars-css": "tsx scripts/implement-css-search-fix.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
